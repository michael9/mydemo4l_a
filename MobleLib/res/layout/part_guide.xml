<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_list" >

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="8dp"
        android:src="@drawable/buttom_log_img" />

    <com.cqvip.moblelib.view.mylinearlayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/myll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <include
            android:id="@+id/guide_title"
            layout="@layout/header" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/secondpagemargin"
            android:background="@drawable/linearlayout_shape1"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/enterguideLayout1"
                android:layout_width="fill_parent"
                android:layout_height="45.0dip"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/guide_enter_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dip"
                    android:paddingRight="20.0dip"
                    android:src="@drawable/icon_content_paste" />

                <TextView
                    android:id="@+id/guide_enter_title"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:duplicateParentState="true"
                    android:gravity="center_vertical"
                    android:text="数据库查询"
                    android:textSize="16sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="20.0dip"
                    android:src="@drawable/eg_arrow" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@drawable/xuxian" />

          <LinearLayout
                android:id="@+id/enterguideLayout2"
                android:layout_width="fill_parent"
                android:layout_height="45.0dip"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/guide_enter_icon2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dip"
                    android:paddingRight="20.0dip"
                    android:src="@drawable/icon_borrow" />

                <TextView
                    android:id="@+id/guide_enter_title2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:duplicateParentState="true"
                    android:gravity="center_vertical"
                    android:text="图书借阅"
                    android:textSize="16sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="20.0dip"
                    android:src="@drawable/eg_arrow" />
            </LinearLayout>

           <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@drawable/xuxian" />

          <LinearLayout
                android:id="@+id/enterguideLayout3"
                android:layout_width="fill_parent"
                android:layout_height="45.0dip"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/guide_enter_icon3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dip"
                    android:paddingRight="20.0dip"
                    android:src="@drawable/icon_book_icon" />

                <TextView
                    android:id="@+id/guide_enter_title3"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:duplicateParentState="true"
                    android:gravity="center_vertical"
                    android:text="期刊借阅"
                    android:textSize="16sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="20.0dip"
                    android:src="@drawable/eg_arrow" />
            </LinearLayout>
 <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@drawable/xuxian" />

          <LinearLayout
                android:id="@+id/enterguideLayout4"
                android:layout_width="fill_parent"
                android:layout_height="45.0dip"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/guide_enter_icon4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dip"
                    android:paddingRight="20.0dip"
                    android:src="@drawable/ic_action_map" />

                <TextView
                    android:id="@+id/guide_enter_title4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:duplicateParentState="true"
                    android:gravity="center_vertical"
                    android:text="文明读者"
                    android:textSize="16sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="20.0dip"
                    android:src="@drawable/eg_arrow" />
            </LinearLayout>
             <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@drawable/xuxian" />

          <LinearLayout
                android:id="@+id/enterguideLayout5"
                android:layout_width="fill_parent"
                android:layout_height="45.0dip"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/guide_enter_icon5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20.0dip"
                    android:paddingRight="20.0dip"
                    android:src="@drawable/icon_device_access_time" />

                <TextView
                    android:id="@+id/guide_enter_title5"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:duplicateParentState="true"
                    android:gravity="center_vertical"
                    android:text="开馆时间"
                    android:textSize="16sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="20.0dip"
                    android:src="@drawable/eg_arrow" />
            </LinearLayout>
              <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@drawable/xuxian" />

    
            
        </LinearLayout>
    </com.cqvip.moblelib.view.mylinearlayout>

</RelativeLayout>